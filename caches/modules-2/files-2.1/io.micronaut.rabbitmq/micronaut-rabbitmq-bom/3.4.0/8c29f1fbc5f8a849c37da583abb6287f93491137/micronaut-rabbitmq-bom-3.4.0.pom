  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>com.rabbitmq</groupId>
        <artifactId>amqp-client</artifactId>
        <version>${amqp.client.version}</version>
      </dependency>
      <dependency>
        <groupId>io.micronaut.rabbitmq</groupId>
