    <connection>scm:git@github.com:micronaut-projects/micronaut-rabbitmq.git</connection>
    <developerConnection>scm:git@github.com:micronaut-projects/micronaut-rabbitmq.git</developerConnection>
  </scm>
  <developers>
    <developer>
      <id>jameskleeh</id>
      <name>James Kleeh</name>
    </developer>
  </developers>
  <properties>
    <micronaut.rabbitmq.version>3.4.0</micronaut.rabbitmq.version>
